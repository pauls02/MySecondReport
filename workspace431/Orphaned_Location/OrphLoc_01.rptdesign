            <method name="open"><![CDATA[
maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

 var sqlText = new String();
 sqlText = 
   "select locations.location,locations.description,locations.siteid,locations.status,locations.glaccount " 
 + "from locations "  
 + "where "  
 + "siteid=locations.siteid "
 + "and orgid=locations.orgid and (glaccount = '??-????-?????' OR glaccount is NULL) and status='OPERATING' and type='OPERATING'  " 
 + "order by locations.location "
 ;


maximoDataSet.setQuery(sqlText);]]></method>
